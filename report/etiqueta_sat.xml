<odoo>
    <data>
        <template id="report_etiqueta_sat_label_cloudalia">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                        <div class="page">
                            <h2>Stock Inventory</h2>

                            <table class="table table-condensed">
                                <thead>
                                    <tr>
                                        <th>
                                            <strong>Inventory</strong>
                                        </th>
                                        <th>
                                            <strong>Date</strong>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            <span t-field="o.id"/>
                                        </td>
                                        <td>
                                            <span t-field="o.create_date"/>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </t>
                </t>
            </t>
        </template>
    </data>
</odoo>