<?xml version="1.0"?>
<odoo>
    <data>
        <record id="helpdesk_ticket_inherit_view_form" model="ir.ui.view">
            <field name="name">helpdesk.ticket.inherit.form.view</field>
            <field name="model">helpdesk.ticket</field>
            <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='x_ordensat']" position="attributes">
                    <attribute name="context">{'name':'id', 'lot_id':'x_sn', 'partner_id':'partner_id'}</attribute>
                </xpath>
            </field>
        </record>
    </data>
</odoo>